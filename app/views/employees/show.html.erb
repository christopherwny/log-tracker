<div class="page-header">
<%= link_to 'Cancel', employees_path, :class=>"btn btn-default pull-right" %>
<%= link_to 'Edit', edit_employee_path(@employee), :class=>"btn btn-default pull-right" %>
<h2><%= @employee.fName %> <%= @employee.lName %><h2>

</div>
<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-xs-8 col-xs-offset-1">
  <strong>First Name:</strong>
  <%= @employee.fName %>
  </div>
</div>
<div class="row">
  <div class="col-xs-8 col-xs-offset-1">
  <strong>Last Name:</strong>
  <%= @employee.lName %>
  </div>
</div>
<div class="row">
  <div class="col-xs-8 col-xs-offset-1">
  <strong>Email Addr:</strong>
  <%= mail_to @employee.email %>
  </div>
</div>
<div class="row">
  <div class="col-xs-8 col-xs-offset-1">
  <strong>Phone #:</strong>
  <%= @employee.phone %>
  </div>
</div>
<div class="row">
  <div class="col-xs-8 col-xs-offset-1">
  <strong>Notes:</strong>
  <%= @employee.notes %>
  </div>
</div>